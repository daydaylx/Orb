# Example Environment Variables
# Copy this file to .env.development or .env.production and adjust values

# App version (auto-populated from package.json)
VITE_APP_VERSION=0.0.0

# Enable debug logging (true/false)
VITE_ENABLE_DEBUG=true

# Maximum number of orbs allowed
VITE_MAX_ORBS=50

# Storage quota warning threshold (0.0 - 1.0)
VITE_STORAGE_QUOTA_WARNING=0.8
